{
  "MongoSettings": {
    "ConnectionString": "__MONGODB_CONNSTRING__",
    "DatabaseName": "datahub-sdi",
    "CollectionName": "discodata_queries"
  },
  "DremioSettings": {
    "Username": "__DREMIO_USER__",
    "Password": "__DREMIO_USER__",
    "DremioServer": "__DREMIO_SERVER__",
    "DremioServerAuth": "__DREMIO_SERVER_AUTH__",
    "Limit": 300000,
    "Timeout" : 120000,
    "MaxResultSetSize": 1000000,
    "DefaultPageSize": 10000,
    "MaxPageSize": 100000,
    "LargeQueryTimeout": 300000,
    "MaxConcurrentQueries": 10,
    "EnableQueryThrottling": true,
    "ThrottleDelayMs": 100
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
